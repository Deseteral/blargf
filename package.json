{
  "name": "blargf",
  "version": "2.0.0",
  "description": "My personal new tab page",
  "main": "build/main.js",
  "scripts": {
    "start": "node ./dist/main.js",
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "npm run lint",
    "lint": "eslint src --ext js,jsx"
  },
  "keywords": [],
  "author": "Deseteral <github.com/deseteral>",
  "license": "MIT",
  "dependencies": {
    "clsx": "1.1.1",
    "compression": "1.7.4",
    "date-fns": "1.30.1",
    "duration": "0.2.2",
    "express": "4.17.0",
    "node-fetch": "2.1.2",
    "node-ical": "0.9.2",
    "number-to-words": "1.2.4",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "shuffle-array": "1.0.1",
    "signale": "1.4.0",
    "styled-components": "4.2.0"
  },
  "devDependencies": {
    "@deseteral/eslint-config": "5.0.1",
    "eslint": "8.1.0",
    "parcel": "2.0.0"
  },
  "targets": {
    "frontend": {
      "source": "src/static/code.js",
      "context": "browser",
      "distDir": "dist/static/"
    },
    "service": {
      "source": "src/main.js",
      "context": "node",
      "distDir": "dist/"
    }
  }
}
